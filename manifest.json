{
    "manifest_version": "1",
    "name": "DonutTab",
    "version": "1",
    "description": "DonutTab Chrome扩展",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "icons/icon19.png",
            "38": "icons/icon38.png"
        },
        "default_title": "DonutTab"
    },
    "content_security_policy": "script-src 'self'; object-src 'self'",
    "chrome_url_overrides": {
        "newtab": "index.html"
    }
}
